{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["server-middleware/**", "api/notify.js"]
      }
    }
  ],
  "routes": [
    {
      "src": "api/notify/callback",
      "dest": "api/notify.js",
      "Access-Control-Allow-Method": "POST",
      "Access-Control-Allow-Headers": "*"
    }
  ]
}